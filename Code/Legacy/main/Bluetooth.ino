#include <SoftwareSerial.h>

const int RX = 11;
const int TX = 10;
SoftwareSerial BlueT(RX,TX);

void bluetooth_setup() {

    // Set up bluetooth
    BlueT.begin(9600);
}

char bluetooth_get_data() {

    if (BlueT.available()) {
        return BlueT.read();
    }
}